{
  "name": "openwiki",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start:prod": "REACT_APP_VERSION=$npm_package_version node server.js",
    "watch-js": "./node_modules/.bin/react-scripts start",
    "start": "REACT_APP_VERSION=$npm_package_version ./node_modules/.bin/npm-run-all -p watch-js",
    "build-js": "./node_modules/.bin/react-scripts build",
    "build": "./node_modules/.bin/npm-run-all build-js",
    "test": "REACT_APP_ENV=test ./node_modules/.bin/react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.6",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.3.0"
  }
}
